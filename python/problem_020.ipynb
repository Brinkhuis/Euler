   "source": [
    "# Factorial\n",
    "def fac(n):\n",
    "    x = 1\n",
    "    for i in range(2, n+1):\n",
    "        x *= i\n",
    "    return x"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "ac3a76dc",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Digit Sum\n",
    "def digit_sum(n):\n",
    "    return sum([int(x) for x in str(n)])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
