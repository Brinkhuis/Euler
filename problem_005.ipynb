   "source": [
    "# Smallest Multiple\n",
    "def smallest_multiple(n):\n",
    "    x = 1\n",
    "    while not all([x%i == 0 for i in range(1, n+1)]):\n",
    "        x += 1\n",
    "    return x"
   ]
